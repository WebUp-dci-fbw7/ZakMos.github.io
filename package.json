{
  "name": "Zakaria-Portfolio",
  "version": "3.0.0",
  "private": true,
  "homepage": "https://ZakMos.github.io",
  "description": "Zakaria's Portfolio",
  "repository": "git@github.com:ZakMos.github.io.git",
  "author": "Zakaria Moslim",
  "license": "MIT",
  "dependencies": {
    "gh-pages": "^4.0.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "build-css": "node-sass-chokidar src/sass -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass -o src/styles --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
