  "homepage": "https://ZakMos.github.io",
  "description": "Zakaria's Portfolio",
  "repository": "git@github.com:ZakMos.github.io.git",
  "author": "Zakaria Moslim",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "bootstrap": "^4.1.3",
    "react": "^16.9.0-rc.0",
    "react-dom": "^16.9.0-rc.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "build-css": "node-sass-chokidar src/sass -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass -o src/styles --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-script": "^2.0.5",
    "react-scripts": "1.1.5"
  }
}
